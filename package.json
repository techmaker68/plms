{
  "name": "plms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite",
    "lint": "eslint src",
    "build-dev": "vite build --mode dev",
    "build-pre": "vite build --mode pre",
    "build-uat": "vite build --mode uat",
    "build-local": "vite build --mode localserver",
    "vue-i18n-extract": "vue-i18n-extract report --noEmptyTranslation=\"*\" --add true --vueFiles=\"./src/**/*.?(vue|ts)\" --languageFiles=\"./src/locales/*.json\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@kouts/vue-modal": "^5.0.0",
    "@vueform/slider": "^2.1.7",
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "@vueuse/core": "^10.2.0",
    "axios": "^1.3.5",
    "bootstrap": "^5.2.3",
    "dayjs": "^1.11.9",
    "docx": "^8.2.0",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "maska": "^2.1.10",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.33.0",
    "quill": "^1.3.7",
    "uuid": "^9.0.1",
    "vue": "^3.2.13",
    "vue-country-flag-next": "^2.3.2",
    "vue-datepicker-next": "^1.0.3",
    "vue-fa": "^4.0.2",
    "vue-i18n": "^9.1.0",
    "vue-router": "^4.1.6",
    "vue-simple-calendar": "^6.3.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-easy-data-table": "^1.5.47",
    "vue3-html2pdf": "^1.1.2",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vitejs/plugin-vue": "^4.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.0",
    "prettier": "2.8.8",
    "pretty-quick": "^3.1.3",
    "rtlcss": "^4.1.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.2.2",
    "vite": "^5.0.1",
    "vue-i18n-extract": "^2.0.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/valid-v-slot": [
        "error",
        {
          "allowModifiers": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
